{
  "function_name": "calendar_agent_api",
  "handler_name": "agent_api_handler.lambda_handler",
  "region_name": "us-east-1",
  "runtime": "python3.13",
  "architecture": "x86_64",
  "timeout": 60,
  "memory_size": 128,
  "role_name": "calendarAgentAPIRole",
  "zip_filename": "calendar_agent_api_lambda.zip",
  "code_files": [
    "__init__.py",
    "agent_api_handler.py",
    "app/__init__.py",
    "app/config.py",
    "app/main.py",
    "app/process_event.py",
    "auth/__init__.py",
    "auth/client_auth.py",
    "auth/slack_auth.py",
    "shared_infrastructure/__init__.py",
    "shared_infrastructure/platform_manager.py",
    "shared_infrastructure/redis_manager.py"
    ],
  "layers": [
    {
      "layer_name": "calendarAgentAPILayer",
      "layer_type": "custom_layer",
      "layer_packages": [
        "redis",
        "slack-sdk"
      ]
    }
  ],
  "test_event_data": {},
  "parameters": {
        "apps/prod/calendar/redis_host": "localhost",
        "apps/prod/calendar/redis_port": 999,
        "apps/prod/calendar/slack_pa_allowed_users": "XXX",
        "apps/prod/calendar/slack_pa_allowed_channels": "XXX",
        "apps/prod/calendar/slack_pa_allowed_bot": "XXX",
        "apps/prod/calendar/slack_pa_integration": "false"

  },
  "secrets": {
      "apps/prod/calendar/secrets/calendar_bearer_token": "XXX",
      "apps/prod/calendar/secrets/redis_password": "XXX",
      "apps/prod/calendar/secrets/slack_pa_signing_secret": "XXX"
  },
  "apigateway_routes": [
    {
      "method": "POST",
      "stage": "prod",
      "path": "/agents/calendar",
      "lambda": "calendar_agent_api"
    }
  ],
  "invoke_lambda_permissions": "calendar_agent"
}

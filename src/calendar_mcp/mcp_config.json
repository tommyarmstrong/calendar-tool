{
  "function_name": "calendar_mcp",
  "handler_name": "mcp_handler.lambda_handler",
  "region_name": "us-east-1",
  "runtime": "python3.13",
  "timeout": 45,
  "memory_size": 128,
  "role_name": "calendarMCPRole",
  "zip_filename": "mcp_calendar_lambda.zip",
  "code_files": [
    "__init__.py",
    "mcp_handler.py",
    "app/__init__.py",
    "app/config.py",
    "app/main.py",
    "auth/__init__.py",
    "auth/google_oauth.py",
    "mcp/__init__.py",
    "mcp/manifest.py",
    "mcp/router.py",
    "mcp/schemas.py",
    "shared_infrastructure/__init__.py",
    "shared_infrastructure/cryptography_manager.py",
    "shared_infrastructure/hmac_auth.py",
    "shared_infrastructure/platform_manager.py",
    "shared_infrastructure/redis_manager.py",
    "tools/__init__.py",
    "tools/calendar.py"
  ],
  "layers": [
    {
      "layer_name": "calendarMCPLayer",
      "layer_type": "custom_layer",
      "layer_packages": [
        "cryptography",
        "google-auth",
        "google-auth-oauthlib",
        "google-api-python-client",
        "pydantic",
        "redis"
      ]
    }
  ],
  "test_event_data": {},
  "parameters": {
        "apps/prod/calendar/calendar_mcp_url": "https://localhost:8000",
        "apps/prod/calendar/google_redirect_uri": "https://localhost:8001/oauth/callback",
        "apps/prod/calendar/google_scopes": [
            "https://www.googleapis.com/auth/calendar.events",
            "https://www.googleapis.com/auth/calendar.readonly"
        ],
        "apps/prod/calendar/redis_host": "localhost",
        "apps/prod/calendar/redis_port": 999
  },
  "secrets": {
        "apps/prod/calendar/secrets/agent_hmac_secret": "XXX",
        "apps/prod/calendar/secrets/calendar_token_encryption_key": "XXX",
        "apps/prod/calendar/secrets/google_client_id": "XXX",
        "apps/prod/calendar/secrets/google_client_secret": "XXX",
        "apps/prod/calendar/secrets/redis_password": "XXX"
  },
  "apigateway_routes": [
    { "method": "GET", "path": "/.well-known/mcp/manifest", "lambda": "calendar_mcp", "stage": "prod" },
    { "method": "GET", "path": "/mcp/schemas", "lambda": "calendar_mcp", "stage": "prod" },
    { "method": "GET", "path": "/mcp/tools", "lambda": "calendar_mcp", "stage": "prod" },
    { "method": "POST", "path": "/mcp/tools/call", "lambda": "calendar_mcp", "stage": "prod" }
  ]
}

{
  "function_name": "calendar_mcp",
  "handler_name": "handler.lambda_handler",
  "region_name": "us-east-1",
  "runtime": "python3.13",
  "timeout": 45,
  "memory_size": 128,
  "role_name": "calendarMCPRole",
  "policy_name": "CalendarMCPPolicy",
  "policy_file": "aws_calendar_mcp_iam_policy.json",
  "zip_filename": "mcp_calendar_lambda.zip",
  "code_files": [
    "__init__.py",
    "handler.py",
    "app/__init__.py",
    "app/config.py",
    "app/main.py",
    "auth/__init__.py",
    "auth/bearer_token_auth.py",
    "auth/google_oauth.py",
    "infrastructure/__init__.py",
    "infrastructure/platform_manager.py",
    "mcp/__init__.py",
    "mcp/manifest.py",
    "mcp/router.py",
    "mcp/schemas.py",
    "services/__init__.py",
    "services/redis_services.py",
    "tools/__init__.py",
    "tools/calendar.py"
  ],
  "layers": [
    {
      "layer_name": "calendarMCPLayer",
      "layer_type": "custom_layer",
      "layer_packages": [
        "cryptography",
        "google-auth",
        "google-auth-oauthlib",
        "google-api-python-client",
        "pydantic",
        "redis"
      ]
    }
  ],
  "test_event_data": {}
}

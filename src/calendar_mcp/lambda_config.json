{
  "function_name": "calendar_mcp",
  "handler_name": "handler.lambda_handler",
  "region_name": "us-east-1",
  "runtime": "python3.13",
  "timeout": 20,
  "memory_size": 128,
  "role_name": "<TBC>",
  "zip_filename": "mcp_lambda.zip",
  "code_files": [
    "__init__.py",
    "handler.py",
    "app/__init__.py",
    "app/config.py",
    "app/main.py",
    "app/middleware.py",
    "auth/__init__.py",
    "auth/google_oauth.py",
    "mcp/__init__.py",
    "mcp/manifest.py",
    "mcp/schema.py",
    "mcp/routers.py",
    "policy/__init__.py",
    "policy/guardrails.py",
    "services/__init__.py",
    "services/aws_platform_services.py",
    "services/redis_services.py",
    "tools/__init__.py",
    "tools/calendar.py"
  ],
  "secrets": [
    "REDIS_PASSWORD",
    "REDIS_HOST",
    "REDIS_PORT"
  ],
  "layers": [
    {
      "layer_name": "calendarMCPServerLayer",
